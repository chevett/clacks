  <script src="/templates.js" type="text/javascript"></script>

  <script type="text/javascript">

      function mt3_renderCommitData(response){
          $("#commit-data-container").append(Handlebars.templates.lastCommit(response));
      }


      $(function(){
          var $txt = $("#txt"), $btn = $("#btn");
          $("#search-container h1").textillate({ in: { effect: 'tada' } });

          $txt.keypress(function(ev){
              if (ev.keyCode == 13){
                  $btn.click();
              }
          })

          $("#btn").click(function(){
            window.location.href = "/"+$txt.val();
          });
      });
  </script>

  <script src="https://api.github.com/repos/chevett/miketown3/git/commits/{{LastCommit}}?callback=mt3_renderCommitData" type="text/javascript"></script>
